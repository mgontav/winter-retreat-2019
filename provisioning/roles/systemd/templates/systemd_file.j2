[Unit]
Description={{ item.value.description }}
Wants=network-online.target
After=network.target network-online.target

[Service]
Type=simple
User={{ item.value.user }}
WorkingDirectory={{ item.value.working_dir }}
ExecStart={{ item.value.executable }}
{% if item.value.restart == true %}
TimeoutSec=15
Restart=always
{% endif %}

[Install]
WantedBy=multi-user.target
